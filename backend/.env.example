# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/aitrader?schema=public"

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Server
PORT=3001
NODE_ENV=development

# DeepSeek AI
DEEPSEEK_API_KEY=sk-your-deepseek-api-key
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# RL Service
RL_SERVICE_URL=http://localhost:8000

# Exchange (Aster)
ASTER_API_KEY=
ASTER_API_SECRET=
ASTER_TESTNET=true

# Trading
TRADING_ENABLED=false
TRADING_MODE=signal
STRATEGY_METHODOLOGY=SMC
TRADING_PAIRS=BTC-USD,ETH-USD

# Risk Management
RISK_MAX_PER_TRADE=0.02
RISK_MAX_DRAWDOWN=0.15
RISK_STOP_LOSS_PCT=0.05
